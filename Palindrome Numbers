palindrome = []
not_Palindrome = []

how_Many = input("How many numbers do you want?: ")

for a in range(int(how_Many)):
    num = input("Enter number: ")
    test = num[::-1] 

    if test == num :
        palindrome.append(int(num))
    
    elif test != num:
        not_Palindrome.append(int(num))

print(palindrome) 
