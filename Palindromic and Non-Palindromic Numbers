palindrome = []
not_Palindrome = []

how_Many = input("How many numbers")

for a in range(int(how_Many)):
    num = input("Enter number: ")
    test = num[::-1] 

    if test == num :
        palindrome.append(int(num))
    
    elif test != num:
        not_Palindrome.append(int(num))

print("Palindrome numbers are : ", palindrome)
print("Non-Palindrome numbers are :", not_Palindrome) 
